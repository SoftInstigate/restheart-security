{
    "configurations": [
        {
            "type": "java",
            "name": "Launch uIAM",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "stopOnEntry": false,
            "mainClass": "io.uiam.Bootstrapper",
            "vmArgs": "--illegal-access=deny -Dfile.encoding=UTF-8 -server",
            "args": "etc/uiam-dev.yml",
            "projectName": "uiam"
        }
    ]
}